{
    "eslint.validate": [
        "javascript",
        "typescript"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "cSpell.words": [
        "Tonomy",
        "tonomy"
    ],
    "conventionalCommits.scopes": [
        "README.md"
    ],
    "git.branchPrefix": "feature/",
    "git.branchProtection": [
        "master",
        "main",
        "development"
    ],
}